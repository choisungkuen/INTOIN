<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="104dp"

    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="104dp"


        >

        <FrameLayout
            android:layout_width="117dp"
            android:layout_height="104dp"

            >

            <ImageView
                android:id="@+id/image_1"
                android:layout_width="117dp"
                android:layout_height="104dp"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"
                android:background="@drawable/ten"
                />
            <ImageButton

                android:id="@+id/bookmark"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"
                android:layout_marginRight="7dp"
                android:layout_marginTop="4dp"
                android:background="@drawable/bookmark_normal"
                android:layout_gravity="right|top" />

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"



            >

            <TextView
                android:layout_marginTop="12dp"
                android:layout_marginLeft="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="인천 쉐르빌 모델하우스"
                android:id="@+id/model_search_item_name"
                android:textColor="#434644"
                android:textSize="14dp"
                />

            <TextView
                android:layout_marginTop="7dp"
                android:layout_marginLeft="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="인천 쉐르빌 모델하우스"
                android:id="@+id/model_search_item_adr"
                android:textColor="#797a78"
                android:textSize="11dp"
                />

            <TextView

                android:layout_marginLeft="17dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="11dp"
                android:textColor="@color/mgm_adr_color"
                android:id="@+id/model_search_item_add"
                android:text="가나다라마바사아차카타파아하가아"
                />








        </LinearLayout>

    </LinearLayout>

</LinearLayout>








